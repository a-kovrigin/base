{% do attributes.removeAttribute('id') %}
{% spaceless %}
<nav{{ attributes.addClass(class) }}>
  {{ title_prefix }}
  {% if label %}
    <p{{ title_attributes.addClass(class ~ '__heading heading') }}>{{ label }}</p>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</nav>
{% endspaceless %}
